import static org.junit.Assert.*;

import org.junit.Test;

public class MensagemBoasVindasTeste {

	@Test
	public void TesteRetornoMensagensJose() {
		MensagemBoasVindas msg = new MensagemBoasVindas();
		String msgRetornoReal = msg.geraMensagem("Jose");
		String msgRetornoEsperado = "Seja bem vindo(a), Jose";
		assertEquals(msgRetornoReal, msgRetornoEsperado);
		
	}

}
